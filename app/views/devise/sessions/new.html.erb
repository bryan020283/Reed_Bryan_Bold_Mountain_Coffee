<center>
  <div class = "jumbotron devise">
    <h2 class = "sign_in">Enter your details to log in</h2>
  </br>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class = "row">
        <div class = "col-md-6">
          <div class="field">
            <%= f.label :email, "Email Address: ", :class => "pull-left" %><br />
            <%= f.email_field :email, autofocus: true, :class => "form-control dev" %>
          </div>
        </div>
        <div class = "col-md-6">
          <div class="field">
            <%= f.label :password, "Password: ", :class => "pull-left" %><br />
            <%= f.password_field :password, autocomplete: "off", :class => "form-control dev" %>
          </div>
        </div>
      </div>

    </br>
        <div class="actions">
          <%= f.submit "Log in", :class => "btn-reg pull-right" %>
        </div>
      </br>
    <% end %>
  </div>
</center>
</br></br></br>